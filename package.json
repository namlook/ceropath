{
  "name": "Ceropath",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "postinstall": "./node_modules/.bin/grunt eureka:install",
    "build-templates": "./node_modules/.bin/grunt build-templates",
    "build": "./node_modules/.bin/grunt build",
    "dist": "(NODE_ENV=production && ./node_modules/.bin/grunt dist)",
    "drop-data": "isql exec=\"SPARQL CLEAR GRAPH 'http://ceropath.org'\"",
    "data-import": "(cd ./scripts &&  ./import-json-data.js > /tmp/data.ttl) && isql exec=\"DB.DBA.TTLP (file_to_string_output ('/tmp/data.ttl'), '', 'http://ceropath.org');\"",
    "live": "npm run build && ./node_modules/.bin/grunt live",
    "test-server": "(NODE_ENV=test && npm run live)",
    "test": "(NODE_ENV=test && npm run build && ./node_modules/.bin/grunt test)",
    "start": "node app/server"
  },
  "author": "namlook <nicolas.clairon@elkorado.com>",
  "dependencies": {
    "eurekapi": "namlook/eurekapi",
    "JSONStream": "^0.8.4"
  },
  "devDependencies": {
    "envify": "^3.0.0",
    "grunt": "^0.4.5",
    "grunt-cli": "^0.1.13",
    "jit-grunt": "^0.8.0",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-browserify": "^2.1.4",
    "grunt-ember-templates": "^0.4.21",
    "grunt-contrib-symlink": "^0.3.0",
    "grunt-bower-install-simple": "^1.0.0",
    "grunt-contrib-concat": "^0.5.0",
    "grunt-contrib-cssmin": "^0.10.0",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-contrib-copy": "^0.5.0",
    "grunt-nodemon": "^0.3.0",
    "grunt-concurrent": "^0.5.0",
    "grunt-contrib-uglify": "^0.5.1",
    "grunt-env": "^0.4.1",
    "grunt-preprocess": "^4.0.0",
    "grunt-extend-config": "^0.9.2",
    "grunt-attention": "0.0.5",
    "grunt-karma": "^0.8.3",
    "mocha": "^1.20.0",
    "chai": "^1.9.1",
    "karma": "^0.12.16",
    "karma-mocha": "^0.1.3",
    "karma-chai": "^0.1.0",
    "karma-phantomjs-launcher": "^0.1.4",
    "karma-safari-launcher": "^0.1.1",
    "karma-firefox-launcher": "^0.1.3",
    "karma-chrome-launcher": "^0.1.4"
  },
  "license": "MIT"
}
